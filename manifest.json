{
  "name": "ニコニコマンダー",
  "version": "1.1",
  "manifest_version": 2,

  "description": "ニコニコ動画視聴ページに便利なショートカットキーと機能を追加します。",

  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],

  "options_page": "options.html",

  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["http://www.nicovideo.jp/*"],
      "js": ["jquery.js", "script.js"]
    }
  ]
}
